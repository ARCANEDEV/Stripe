{
    "name": "arcanedev/stripe",
    "description": "Stripe PHP Library",
    "keywords": ["stripe", "payment", "pay", "api", "library", "arcanedev"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stripe and contributors",
            "homepage": "https://github.com/stripe/stripe-php/contributors"
        },
        {
            "name": "ARCANEDEV",
            "homepage": "https://github.com/ARCANEDEV/Stripe/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "phpunit/phpcov": "~2.0",
        "mockery/mockery": "~0.9",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Stripe\\": "src/"
        },
        "files": ["src/start.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Stripe\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
